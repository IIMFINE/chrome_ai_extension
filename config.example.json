{
  "apiProvider": "openai",
  "apiEndpoint": "https://api.openai.com/v1/chat/completions",
  "models": {
    "gpt-4": "GPT-4",
    "gpt-4o": "GPT-4o",
    "gpt-4o-mini": "GPT-4o Mini",
    "gpt-4-turbo-preview": "GPT-4 Turbo",
    "gpt-3.5-turbo": "GPT-3.5 Turbo"
  },
  "defaultModel": "gpt-4",
  "settings": {
    "temperature": 0.7,
    "maxTokens": 2000,
    "maxContentLength": 10000
  },
  "ui": {
    "theme": "light",
    "language": "zh-CN"
  },
  "note": "支持任何 OpenAI 兼容的 API endpoint，如 Azure OpenAI、本地部署的模型等"
}
